<xsd:schema xmlns="http://www.lmonte.com/prop39/UtilDataProp39Report" xmlns:util="http://www.lmonte.com/prop39/util" xmlns:dtyp="http://www.lmonte.com/prop39/dtyp" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:altova="http://www.altova.com/xml-schema-extensions" targetNamespace="http://www.lmonte.com/prop39/UtilDataProp39Report" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.004">
	<xsd:import namespace="http://www.lmonte.com/prop39/util" schemaLocation="UtilityDataModel.xsd"/>
	<xsd:import namespace="http://www.lmonte.com/prop39/dtyp" schemaLocation="DataTypes.xsd"/>
	<xsd:annotation>
