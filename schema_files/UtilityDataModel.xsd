		<xsd:annotation>
			<xsd:documentation>This uniquely identifiers the physical meter delivering usage data to ta usage point.</xsd:documentation>
			<xsd:appinfo source="BEDES">
				<dtyp:BEDESTerm>Utility Meter Number</dtyp:BEDESTerm>
			</xsd:appinfo>
			<xsd:appinfo source="GreenButton">
				<dtyp:GreenButtonTerm>retailCustomer.xsd: EndDevice - utcNumber</dtyp:GreenButtonTerm>
			</xsd:appinfo>
			<xsd:appinfo source="PGE">
				<dtyp:PGETerm>Mtr_Id</dtyp:PGETerm>
			</xsd:appinfo>
			<xsd:appinfo source="SCE">
				<dtyp:SCETerm>Meter Number</dtyp:SCETerm>
			</xsd:appinfo>
