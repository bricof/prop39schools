			<xsd:documentation>Maximum demand during the peak time of use period during the annual reporting period.  Units: none</xsd:documentation>
			<xsd:appinfo source="BEDES">
				<dtyp:BEDESTerm>Resource Use On Peak Maximum Annual: COMPOSITE: Interval Frequency = "Annual", Meter Reading Type = "On peak", Meter Reading Type = "Maximum",  Resource Use = [value]</dtyp:BEDESTerm>
			</xsd:appinfo>
			<xsd:appinfo source="GreenButton">
				<dtyp:GreenButtonTerm>espiDerived.xsd: IntervalReading - EnergyUsage combined with ReadingType -  tou</dtyp:GreenButtonTerm>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="dtyp:DecimalNonnegative"/>
	</xsd:simpleType>
	<xsd:simpleType name="DemandMaximumSemiPeak">
		<xsd:annotation>
			<xsd:documentation>Maximum demand during semi peak or mid peak time of use period.  Units: none</xsd:documentation>
			<xsd:appinfo source="BEDES">
				<dtyp:BEDESTerm>Resource Use Semi Peak Maximum: COMPOSITE: Interval Frequency = "Billing Period", Meter Reading Type = "Semi peak", Meter Reading Type = "Maximum",  Resource Use = [value]</dtyp:BEDESTerm>
			</xsd:appinfo>
			<xsd:appinfo source="GreenButton">
				<dtyp:GreenButtonTerm>espiDerived.xsd: IntervalReading - EnergyUsage combined with ReadingType -  tou</dtyp:GreenButtonTerm>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="dtyp:DecimalNonnegative"/>
	</xsd:simpleType>
	<xsd:simpleType name="DemandMaximumSemiPeakAnnual">
		<xsd:annotation>
			<xsd:documentation>Maximum demand during semi peak or mid peak time of use period during the annual reporting period.  Units: none</xsd:documentation>
			<xsd:appinfo source="BEDES">
				<dtyp:BEDESTerm>Resource Use Semi Peak Maximum Annual: COMPOSITE: Interval Frequency = "Annual", Meter Reading Type = "Semi peak", Meter Reading Type = "Maximum",  Resource Use = [value]</dtyp:BEDESTerm>
			</xsd:appinfo>
			<xsd:appinfo source="GreenButton">
				<dtyp:GreenButtonTerm>espiDerived.xsd: IntervalReading - EnergyUsage combined with ReadingType -  tou</dtyp:GreenButtonTerm>
			</xsd:appinfo>
