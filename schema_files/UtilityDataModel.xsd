	<xsd:simpleType name="P39AnnualPeriodEndDate">
		<xsd:annotation>
			<xsd:documentation>This is the end date for an annual reporting period for the Proposition 39 school utility data.  Its required format is yyyy-mm-dd and is controlled to be June 30 of a year.  The first possible end date is 2013-06-30.</xsd:documentation>
			<xsd:appinfo source="BEDES">
				<dtyp:BEDESTerm>End Time Stamp</dtyp:BEDESTerm>
			</xsd:appinfo>
			<xsd:appinfo source="GreenButton">
				<dtyp:GreenButtonTerm>n/a</dtyp:GreenButtonTerm>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="dtyp:EnumerationDateConstant">
			<xsd:enumeration value="2013-06-30"/>
			<xsd:enumeration value="2014-06-30"/>
			<xsd:enumeration value="2015-06-30"/>
			<xsd:enumeration value="2016-06-30"/>
			<xsd:enumeration value="2017-06-30"/>
			<xsd:enumeration value="2018-06-30"/>
			<xsd:enumeration value="2019-06-30"/>
			<xsd:enumeration value="2020-06-30"/>
			<xsd:enumeration value="2021-06-30"/>
			<xsd:enumeration value="2022-06-30"/>
			<xsd:enumeration value="2023-06-30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="P39AnnualPeriodStartDate">
		<xsd:annotation>
			<xsd:documentation>This is the begin date for an annual reporting period for the Proposition 39 school utility data.  Its required format is yyyy-mm-dd and is controlled to be July 01 of a year.  The first possible begin date is 2012-07-30.</xsd:documentation>
			<xsd:appinfo source="BEDES">
				<dtyp:BEDESTerm>Start Time Stamp</dtyp:BEDESTerm>
			</xsd:appinfo>
			<xsd:appinfo source="GreenButton">
				<dtyp:GreenButtonTerm>n/a</dtyp:GreenButtonTerm>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="dtyp:EnumerationDateConstant">
			<xsd:enumeration value="2012-07-01"/>
			<xsd:enumeration value="2013-07-01"/>
			<xsd:enumeration value="2014-07-01"/>
